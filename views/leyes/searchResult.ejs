<!DOCTYPE html>
<html lang="es">
<!-- Head -->
<%- include('../partials/head', {title: 'Resultados BÃºsqueda'}) %>
	<body>
		<!-- header -->
		<%- include('../partials/headerNavbar') %>
		<!-- /header -->
		<div class="container-fluid background-wrapper">
			<div class="container result-list">
				<h3>Resultados</h3>			
				<% if (data) { %>
					<% data.forEach(element => { %>
						<ul>
							<li>Nombre: <%= element.entityTypes.name %> - <%= element.number %> - <%= element.year %></li>
							<a href="/leyes/detail/<%= element.id %>">
								Ver Detalle
							</a>
						</ul>
					<% }) %>
				<% } %>
				<button type="button" class="btn btn-info btn-sm" onclick="history.back()">Volver</button>
			</div>
		</div>
		<!-- footer -->
		<%- include('../partials/footer') %>
		<!-- /footer -->
  </body>
</html>
